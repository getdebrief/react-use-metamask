{
  "name": "@navvi/react-use-metamask",
  "version": "0.1.8",
  "description": "Metamask hook for react apps",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "react",
    "reactjs",
    "metamask",
    "web3"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:cover": "jest --coverage",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
    "lint": "npx eslint --ext .ts --ext .tsx --max-warnings 0 .",
    "lint-fix": "npx eslint --ext .ts --ext .tsx --max-warnings 0 --fix ./src",
    "prepublish": "npm run build"
  },
  "author": "ned@navvi.xyz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/getdebrief/react-use-metamask.git"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^8.0.0",
    "@types/chai": "^4.3.0",
    "@types/jest": "^27.4.1",
    "@types/mocha": "^9.1.0",
    "@types/react": "^17",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "assert": "^2.0.0",
    "autoprefixer": "^10.4.2",
    "chai": "^4.3.6",
    "eslint": "8.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eth-testing": "^1.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.6",
    "prettier": "^2.6.0",
    "react": "^17",
    "ts-jest": "^27.1.4",
    "typescript": "4.5.5"
  },
  "peer-dependencies": {
    "react": ">=17.x"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "npx eslint --ext .ts --ext .tsx --fix --max-warnings 0 --"
    ]
  }
}
